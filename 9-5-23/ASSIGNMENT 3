t=int(input())
s=0
while(t>0):
    import math
    l=[]
    n=int(input())
    while(n%2==0):
        l.append(2)
        n=n//2
    for i in range(3,int(math.sqrt(n))+1,2):
        while(n%i==0):
            l.append(i)
            n=n//i
    if n>2:
        l.append
        (n)
    t=t-1
    for i in l:
        s=s+i
print(s)
    
