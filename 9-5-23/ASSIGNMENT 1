import math
l=[]
n=int(input())
while(n%2==0):
    l.append(2)
    n=n//2
for i in range(3,int(math.sqrt(n))+1,2):
    while(n%i==0):
        l.append(i)
        n=n//i
s=0
if n>2:
    l.append(n)
print(len(l),end='(')
for i in range(0,len(l)):
    if i==len(l)-1:
        print(l[i],end='')
    else:
        print(l[i],end=',')   
    s=s+l[i]
print(')')
print(s
