class NumArray:
        def __init__(self, nums: List[int]):
        n=len(nums)
        self.p=[0]*(n+1)
        self.p[0]=nums[0]
        for i in range(1,n):
            self.p[i]=self.p[i-1]+nums[i]

    def sumRange(self, left: int, right: int) -> int:
        self.r=self.p[right]-self.p[left-1]
        return self.r
